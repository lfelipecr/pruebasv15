<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_account_journal_inherit_form" model="ir.ui.view">
        <field name="model">account.journal</field>
        <field name="inherit_id" ref="account.view_account_journal_form"/>
        <field name="arch" type="xml">
            <field name="type" position="after">
                <field name="e_invoice_check" groups="l10n_cr_places.module_einvoice_costa_rica"/>
                 <field name="to_send" groups="l10n_cr_places.module_einvoice_costa_rica"/>
            </field>
        </field>
    </record>

     <record id="view_account_journal_inherit_tree" model="ir.ui.view">
        <field name="model">account.journal</field>
        <field name="inherit_id" ref="account.view_account_journal_tree"/>
        <field name="arch" type="xml">
            <field name="type" position="after">
                <field name="e_invoice_check" groups="l10n_cr_places.module_einvoice_costa_rica"/>
                <field name="to_send" groups="l10n_cr_places.module_einvoice_costa_rica"/>
            </field>
        </field>
    </record>
</odoo>
